# Home-Health 生产环境变量配置
# 复制此文件为 .env 并填入实际值

# GitHub 用户名或组织名（用于构建镜像）
GITHUB_USERNAME=wewejl

# 镜像标签（latest 或 Git SHA）
IMAGE_TAG=latest

# PostgreSQL 数据库配置
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=home_health

# JWT 认证配置
JWT_SECRET_KEY=your_jwt_secret_key_here_change_this
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=480

# 阿里云通义千问配置
LLM_API_KEY=your_qwen_api_key
LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
LLM_MODEL=qwen-plus
QWEN_VL_MODEL=qwen3-vl-plus

# 阿里云短信服务配置
ALIYUN_SMS_ACCESS_KEY_ID=your_aliyun_access_key
ALIYUN_SMS_ACCESS_KEY_SECRET=your_aliyun_secret_key
